{
  "name": "dgwltd",
  "version": "1.0.27",
  "description": "## Requirements",
  "main": "gulpfile.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "clean:theme:css": "trash \"./public/css/**\"",
    "clean:admin:css": "trash \"./admin/css/**\"",
    "clean:blocks": "trash \"./dist/blocks/**\"",
    "clean": "run-s clean:*",
    "css:theme": "sass ./src/scss/dgwltd-site-theme.scss ./public/css/dgwltd-site-theme.css",
    "css:editor": "sass ./src/scss/dgwltd-site-editor.scss ./admin/css/dgwltd-site-editor.css",
    "postcss:theme": "postcss ./public/css/dgwltd-site-theme.css --use autoprefixer postcss-custom-properties cssnano --map --verbose --dir ./public/css/",
    "postcss:editor": "postcss ./admin/css/dgwltd-site-editor.css --use autoprefixer postcss-custom-properties cssnano --map --verbose --dir ./admin/css/",
    "scripts:build": "wp-scripts build ./admin/scripts/dgwltd-site-editor.js ./admin/scripts/dgwltd-site-variations.js ./public/scripts/dgwltd-site-theme.js --output-path=./dist",
    "blocks:develop": "wp-scripts start --webpack-src-dir=./src/blocks/src --webpack-copy-php --output-path=./src/blocks/build",
    "blocks:build": "wp-scripts build --webpack-src-dir=./src/blocks/src --webpack-copy-php --output-path=./src/blocks/build",
    "watch": "onchange \"**/*.scss\" -- npm run css:* --silent",
    "develop": "concurrently --kill-others \"npm run watch\" \"npm run blocks:develop\"",
    "build": "NODE_ENV=production run-p clean:* && NODE_ENV=production run-s css:* postcss:* && NODE_ENV=production && run-s scripts:* && NODE_ENV=production run-s blocks:build && NODE_ENV=production run-s clean && NODE_ENV=production $(npm version patch --no-git-tag-version)"
  },
  "dependencies": {
    "@10up/block-components": "^1.19.3",
    "@alpinejs/focus": "^3.14.1",
    "@alpinejs/intersect": "^3.14.1",
    "@alpinejs/ui": "^3.14.3-beta.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@wordpress/icons": "^10.9.0",
    "@wordpress/scripts": "^26.19.0",
    "alpinejs": "^3.14.5",
    "autoprefixer": "^10.4.5",
    "concat": "^1.0.3",
    "concurrently": "^5.3.0",
    "cssnano": "^5.0.8",
    "lite-vimeo-embed": "^0.3.0",
    "lite-youtube-embed": "^0.3.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.3.6",
    "postcss-cli": "^10.0.0",
    "postcss-custom-properties": "^11.0.0",
    "postcss-scss": "^4.0.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "sass": "^1.49.10",
    "trash-cli": "^5.0.0",
    "uglify-js": "^3.14.2"
  }
}
